#build environment
export CC="gcc"
export CFLAGS="-O2 -ggdb -pipe "
export CPP="gcc -E"
export CPPFLAGS="-D_XOPEN_SOURCE=600 -D_XOPEN_SOURCE_EXTENDED -D_BSD_SOURCE -D_DEFAULT_SOURCE -P -DDEBUG=0 -D_FORTIFY_SOURCE=2"
export LDFLAGS=""
export SOCKDDEPS="-lm -lwrap"
export LIBS="-lpam -lnsl -lcrypt  -ldl "
